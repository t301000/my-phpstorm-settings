<templateSet group="user">
  <template name="!funexist" value="if (! function_exists('$funcName$')) {&#10;    $END$&#10;}" description="if function not exist" toReformat="true" toShortenFQNames="true">
    <variable name="funcName" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Expression" value="true" />
      <option name="PHP Statement" value="true" />
    </context>
  </template>
</templateSet>